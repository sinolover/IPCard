Private Sub Form_Load()
Dim strSql As String
blnFrmOpened = True
setADODC Me.adcCount
RefreshWindows Me.dgCount.Columns
strAlert = GetReg("AlertDate", "3")
If Not IsNumeric(strAlert) Then strAlert = "3"
strSql = "Select * from cxAlertMoney where trim(WkrName)='" _
         & Trim(strOprName) & "' and LastDate <= # " _
         & Format(DateAdd("d", Val(strAlert) * (-1), Date), "YYYY-MM-DD") _
         & " # order by LastDate "
adcCount.RecordSource = strSql
adcCount.Refresh
If Not adcCount.Recordset.EOF Then adcCount.Recordset.MoveLast
End Sub
